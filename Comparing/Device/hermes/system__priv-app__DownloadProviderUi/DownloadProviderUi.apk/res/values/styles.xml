<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="CircularProgressBar">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="stroke_width">5.0dip</item>
        <item name="progress">0.1</item>
        <item name="progress_color">#ffff4444</item>
        <item name="progress_background_color">#ff898989</item>
        <item name="thumb_visible">false</item>
        <item name="marker_visible">false</item>
    </style>
    <style name="CircularProgressBarLight">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="stroke_width">5.0dip</item>
        <item name="progress">0.1</item>
        <item name="progress_color">#ffff4444</item>
        <item name="progress_background_color">#ffaaaaaa</item>
        <item name="thumb_visible">false</item>
        <item name="marker_visible">false</item>
    </style>
    <style name="ActionBar.ShowCustom" parent="@miui:style/Widget.ActionBar">
        <item name="android:displayOptions">showCustom</item>
    </style>
    <style name="ActionBar.TabBar" parent="@miui:style/Widget.ActionBar.TabBar">
        <item name="android:background">@drawable/action_bar_bg_light</item>
    </style>
    <style name="ActionBar.TabView" parent="@miui:style/Widget.ActionBar.TabView">
        <item name="android:gravity">center|left</item>
        <item name="android:background">@drawable/action_bar_tab_bg_light</item>
    </style>
    <style name="Filter.Text.style" parent="@miui:style/TextAppearance.List.Primary">
        <item name="android:textStyle">normal</item>
    </style>
    <style name="Theme.Light.Immersion" parent="@miui:style/Theme.Light">
        <item name="android:actionBarStyle">@style/ActionBar.ShowCustom</item>
        <item name="miui:immersionMenuEnabled">true</item>
    </style>
    <style name="MyActionBarTabText" parent="@miui:style/Widget.ActionBar.TabText">
        <item name="android:textColor">@color/download_list_title_text_color</item>
    </style>
    <style name="sc_TransparentTheme" parent="@android:style/Theme">
        <item name="android:windowBackground">@color/focus_color</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@null</item>
    </style>
    <style name="TextAppearanceMainTitle">
        <item name="android:textSize">19.0dip</item>
        <item name="android:textColor">@android:color/white</item>
    </style>
    <style name="AnimationActivity" parent="@android:style/Animation.Activity">
        <item name="android:activityOpenEnterAnimation">@null</item>
        <item name="android:activityOpenExitAnimation">@null</item>
        <item name="android:activityCloseEnterAnimation">@null</item>
        <item name="android:activityCloseExitAnimation">@null</item>
        <item name="android:taskOpenEnterAnimation">@null</item>
        <item name="android:taskOpenExitAnimation">@null</item>
        <item name="android:taskCloseEnterAnimation">@null</item>
        <item name="android:taskCloseExitAnimation">@null</item>
        <item name="android:taskToFrontEnterAnimation">@null</item>
        <item name="android:taskToFrontExitAnimation">@null</item>
        <item name="android:taskToBackEnterAnimation">@null</item>
        <item name="android:taskToBackExitAnimation">@null</item>
    </style>
    <style name="ThemeActivity" parent="@android:style/Theme.NoTitleBar">
        <item name="android:windowBackground">@android:color/white</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@style/AnimationActivity</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
    </style>
    <style name="Widget.ListView.Item.Title" parent="@miui:style/Widget.ListView.Item.DoubleLine">
        <item name="android:layout_height">@dimen/download_item_title_height</item>
        <item name="android:minHeight">@dimen/download_item_title_height</item>
    </style>
    <style name="FilePickerTheme" parent="@miui:style/Theme.Light">
        <item name="android:windowIsTranslucent">true</item>
    </style>
    <style name="PathItemTextStyle">
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/path_item_text_color</item>
    </style>
    <style name="FileListItemStyle">
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">14.0dip</item>
        <item name="android:paddingRight">14.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/list_preferred_item_height</item>
        <item name="android:minHeight">@dimen/list_preferred_item_height</item>
    </style>
    <style name="custom_dialog" parent="@android:style/Theme.Holo.DialogWhenLarge">
        <item name="android:windowBackground">@drawable/white</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@style/dialog_animation</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>
    <style name="dialog_animation" parent="@android:style/Animation.Dialog">
        <item name="android:windowEnterAnimation">@anim/sniff_push_bottom_in</item>
        <item name="android:windowExitAnimation">@anim/sniff_push_bottom_out</item>
    </style>
    <item type="style" name="APKTOOL_DUMMY_0012">false</item>
    <style name="Theme.Light.NoTitile.Transparent" parent="@miui:style/Theme.Light.NoTitle">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
    </style>
    <item type="style" name="APKTOOL_DUMMY_0014">false</item>
</resources>
